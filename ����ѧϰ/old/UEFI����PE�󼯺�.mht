function seditor_insertunit(key, text, textend, moveend, selappend) {
	if($(key + 'message')) {
		$(key + 'message').focus();
	}
	textend =3D isUndefined(textend) ? '' : textend;
	moveend =3D isUndefined(textend) ? 0 : moveend;
	selappend =3D isUndefined(selappend) ? 1 : selappend;
	startlen =3D strlen(text);
	endlen =3D strlen(textend);
	if(!isUndefined($(key + 'message').selectionStart)) {
		if(selappend) {
			var opn =3D $(key + 'message').selectionStart + 0;
			if(textend !=3D '') {
				text =3D text + $(key + 'message').value.substring($(key + =
'message').selectionStart, $(key + 'message').selectionEnd) + textend;
			}
			$(key + 'message').value =3D $(key + 'message').value.substr(0, $(key =
+ 'message').selectionStart) + text + $(key + =
'message').value.substr($(key + 'message').selectionEnd);
			if(!moveend) {
				$(key + 'message').selectionStart =3D opn + strlen(text) - endlen;
				$(key + 'message').selectionEnd =3D opn + strlen(text) - endlen;
			}
		} else {
			text =3D text + textend;
			$(key + 'message').value =3D $(key + 'message').value.substr(0, $(key =
+ 'message').selectionStart) + text + $(key + =
'message').value.substr($(key + 'message').selectionEnd);
		}
	} else if(document.selection && document.selection.createRange) {
		var sel =3D document.selection.createRange();
		if(!sel.text.length && $(key + 'message').sel) {
			sel =3D $(key + 'message').sel;
			$(key + 'message').sel =3D null;
		}
		if(selappend) {
			if(textend !=3D '') {
				text =3D text + sel.text + textend;
			}
			sel.text =3D text.replace(/\r?\n/g, '\r\n');
			if(!moveend) {
