--dhcp-script=3D/usr/bin/nova-dhcpbr
idge --leasefile-ro
root      2730  0.0  0.0  27504   240 ?        S    23:12   0:00 =
/usr/sbin/dns
masq --strict-order --bind-interfaces --conf-file=3D =
--domain=3Dnovalocal --pid-fi
le=3D/var/lib/nova/networks/nova-br100.pid --listen-address=3D10.0.0.1 =
--except-in
terface=3Dlo --dhcp-range=3D10.0.0.2,static,120s --dhcp-lease-max=3D256 =
--dhcp-hosts
file=3D/var/lib/nova/networks/nova-br100.conf =
--dhcp-script=3D/usr/bin/nova-dhcpbr
